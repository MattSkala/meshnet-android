<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <EditTextPreference
        app:key="username"
        app:title="Username" />

    <ListPreference
        app:key="connectivity"
        app:title="Connectivity Manager"
        app:entries="@array/connectivity_entries"
        app:entryValues="@array/connectivity_values"
        app:defaultValue="NearbyConnectivityManager" />

</PreferenceScreen>
